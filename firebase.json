{
  "hosting": {
    "public": "_site",
    "ignore": [
      "firebase.json",
      "**/.*"
    ],
    "headers": [ {
    "source": "**/*.@(jpg|gif|png|webp|css|js|ico)",
    "headers": [ {
      "key": "Cache-Control",
      "value": "public, max-age=31536000"
    } ]
  }, {
    "source": "**/*.@(jpg|gif|png|webp|css|js|ico)",
    "headers": [ {
      "key": "Expires",
      "value": "2030-12-31 00:00:00 -0000"
    } ]
  } ]
  }
}
